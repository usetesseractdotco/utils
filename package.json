{
  "name": "@tesseract/utils",
  "description": "A collection of utilities for the Tesseract project",
  "author": "Tesseract Team",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "bun --watch src/index.ts",
    "lint": "eslint src --ext .ts --fix",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "authentication",
    "security",
    "utilities",
    "totp",
    "password",
    "session",
    "cache",
    "crypto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/usetesseractdotco/utils.git"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bun": "latest",
    "@types/qrcode": "^1.5.5",
    "eslint": "8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "qrcode": "^1.5.4",
    "zod": "^3.24.4"
  }
}
